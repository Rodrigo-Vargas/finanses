<!DOCTYPE html>
<html>
  <head>
    <title>Finanses</title>
    <%= csrf_meta_tags %>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular.min.js"></script>
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular-cookies.js"></script>

    <script type="text/javascript" src="/app.js"></script>
    <%= javascript_include_tag 'application' %>

  </head>

  <body ng-app="finansesApp">
    <div class="container-fluid">
      <%= content_for?(:content) ? yield(:content) : yield %>
    </div>
  </body>
</html>
